[inet]
inetrouter ansible_host=127.0.0.1 ansible_port=2222 ansible_private_key_file=.vagrant/machines/inetrouters/virtualbox/private_key
[inter]
centralrouter ansible_host=127.0.0.1 ansible_port=2200 ansible_private_key_file=.vagrant/machines/centralrouter/virtualbox/private_key
[servers]
centralserver ansible_host=127.0.0.1 ansible_port=2201 ansible_private_key_file=.vagrant/machines/centralserver/virtualbox/private_key
